<h1>New User. Welome to the Music Sharing Website</h1>

<p>Please sign up here.</p>

 <%= render 'shared/errors' %>

<form action=" <%= users_url %> " method="post">

  <label for="email">Email Address:</label>
  <input id="email" type="text" name="user[email]">

  <label for="password">Password(Must be six characters)</label>
  <input id="password" type="text" name="user[password]">

  <input type="hidden"
         name="authenticity_token"
         value=" <%= form_authenticity_token %> ">

  <input type="submit" value="Sign Up">
</form>
